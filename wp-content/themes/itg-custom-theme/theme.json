{
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": true,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#10416f",
					"name": "iTG Blue",
					"slug": "custom-i-tg-blue"
				},
				{
					"color": "#497394",
					"name": "ITG Med Blue",
					"slug": "custom-itg-med-blue"
				},
				{
					"color": "#90b2c4",
					"name": "iTG Lt Blue",
					"slug": "custom-i-tg-lt-blue"
				},
				{
					"color": "#dcf0fa",
					"name": "ITG Really Lt Blue",
					"slug": "custom-itg-really-lt-blue"
				}
			]
		},
		"layout": {
			"contentSize": "960px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Sacramento",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/sacramento/sacramento-400-normal.woff2"
							]
						}
					],
					"fontFamily": "Sacramento, cursive",
					"name": "Sacramento",
					"slug": "sacramento"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Alice",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/alice/alice-400-normal.woff2"
							]
						}
					],
					"fontFamily": "Alice, serif",
					"name": "Alice",
					"slug": "alice"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Roboto",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/roboto/roboto-400-normal.woff2"
							]
						},
						{
							"fontFamily": "Roboto",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/roboto/roboto-700-normal.woff2"
							]
						},
						{
							"fontFamily": "Roboto",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/roboto/roboto-400-italic.woff2"
							]
						}
					],
					"fontFamily": "Roboto, sans-serif",
					"name": "Roboto",
					"slug": "roboto"
				}
			],
			"fontSizes": []
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--custom-itg-really-lt-blue)",
			"text": "var(--wp--preset--color--custom-i-tg-blue)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--custom-i-tg-blue)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontStyle": "normal",
					"fontWeight": "700"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--custom-i-tg-blue)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--alice)"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--custom-i-tg-blue)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontStyle": "normal",
					"fontWeight": "700"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--custom-i-tg-blue)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--custom-itg-med-blue)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)"
				}
			}
		},
		"spacing": {
			"padding": {
				"bottom": "0px",
				"left": "1rem",
				"right": "1rem",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--roboto)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json"
}